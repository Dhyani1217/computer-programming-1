#include <stdio.h>
#include <conio.h>

void main()
{
    float H, L, A;

    clrscr();

    printf("Enter height of triangle: ");
    scanf("%f", &H);

    printf("Enter base of triangle: ");
    scanf("%f", &L);

    A = (H * L) / 2;

    printf("Area of Triangle = %.2f", A);

    getch();
}
